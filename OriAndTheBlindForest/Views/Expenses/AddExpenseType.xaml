        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OriAndTheBlindForest.Views.Expenses"
        mc:Ignorable="d"
        Title="AddExpenseType" Height="450" Width="800">
    <Grid>
        <Label Content="Type New Expense Type" HorizontalAlignment="Left" Height="39" Margin="67,73,0,0" VerticalAlignment="Top" Width="164"/>
        <TextBox HorizontalAlignment="Left" Height="39" Margin="248,73,0,0" TextWrapping="Wrap" Text="Expense Type" VerticalAlignment="Top" Width="193"/>
        <Button Name="saveExpenseTypeButton" Content="Save" HorizontalAlignment="Left" Height="49" Margin="248,168,0,0" VerticalAlignment="Top" Width="193" Click="saveExpenseTypeButton_Click"/>
        <Button x:Name="naruSwitch" Content="Switch to Naru" HorizontalAlignment="Left" Height="48" Margin="518,10,0,0" VerticalAlignment="Top" Width="245"/>

    </Grid>
</Window>
